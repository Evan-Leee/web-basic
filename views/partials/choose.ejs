<%var data = dataArray['choose'];%>

<div>
    <h3><%=data.title%></h3>
    <% data.content.forEach(function(element,i){%>

        <label><%=i+1%>.<%=element.description%>:</label>
        <ul>
            <%element.items.forEach(function(item){%>

            <li><input type="radio" name="<%=element.name%>" value="<%=item[0]%>"><%=item[1]%></li>

                    <%})%>
        </ul>
    <%});%>

</div>